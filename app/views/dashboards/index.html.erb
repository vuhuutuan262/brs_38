<div class="container">
  <div class="row col-md-offset-3">
    <h2><%= t "dashboard.toprequest" %></h2>
  </div>
  <div class="row">
    <% if @requests.empty? %>
      <%= t "dashboard.requestempty" %>
    <% else %>
      <%= render "dashboards/request_top_form" %>
    <% end %>
  </div>
  <div class="row col-md-offset-3">
    <h2><%= t "dashboard.topuser" %></h2>
  </div>
  <div class="row">
    <%= render "dashboards/user_top_form" %>
  </div>
</div>
